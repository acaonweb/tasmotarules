!mem1 low value
!mem2 high value
!set from another tasmota device
!publish2 /cmnd/%topic%/mem1 lowvalue
!publish2 /cmnd/%topic%/mem highvalue
!
!disable DeepSleepTime 
!publish2 /cmnd/%topic%/DeepSleepTime 0, remember to remove retained message --> publish2 /cmnd/%topic%/DeepSleepTime
!
!

!idx 184 index of soil moisture virtual dummy sensor in domoticz, idx 185 text dummy sensor
rule1 
ON analog#a0 DO 
   Backlog scale1 %value%, %mem2%, %mem1% ,100,0; publish domoticz/in {"idx":184,"nvalue": %var1% }
ENDON
ON System#Save DO 
   Backlog scale2 %var1%, 100,0 ,%mem2%,%mem1%; publish domoticz/in {"idx":185,"nvalue": %var2% }
ENDON
ON 


rule1 
ON tele-analog#a0 DO 
   Backlog scale1 %value%, %mem2%, %mem1% ,100,0; publish domoticz/in {"idx":184,"nvalue": %var1% }
ENDON
